{
    "plugins": ["prettier-plugin-multiline-arrays"],
    "multilineArraysWrapThreshold": 1,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 80,
    "endOfLine": "lf"
}
